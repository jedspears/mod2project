<h1>Log In</h1>

<%if flash[:notice]%>
  <%=flash[:notice]%>
<%end%>

<%= form_tag "/confirm", method: "get" do%>
  <%= label_tag(:username, "Username:") %>
  <%= text_field_tag(:username) %><br>
  <%= label_tag(:password, "Password:") %>
  <%= text_field_tag(:password) %><br>
  <%= submit_tag("Log In") %>
<% end %>
