<%@user.bets.each do |bet|%>
  <%=link_to bet_path(bet) do %>
    <div>
      Pot: $<%=bet.pot%><br>
      Participants:
      <ul>
        <%bet.get_users.each do |user|%>
          <li><%=user.username%> </li>
        <%end%>
      </ul>
      Description: <%=bet.description%>
    </div><br>
  <%end%>
<%end%>
