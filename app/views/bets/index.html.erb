<%@user.transacktions.each do |transacktion|%>
  <%=link_to bet_path(transacktion.bet) do %>
    <div>
      Pot: $<%=transacktion.bet.pot%><br>
      Participants:
      <ul>
        <%transacktion.bet.get_users.each do |user|%>
          <li><%=user.username%> </li>
        <%end%>
      </ul>
      Description: <%=transacktion.bet.description%>
    </div><br>
  <%end%>
<%end%>
